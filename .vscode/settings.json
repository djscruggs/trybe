{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
},
  "eslint.validate": ["javascript","typescript"],
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.enable": true,
  "[prisma]": {
  "editor.defaultFormatter": "Prisma.prisma"
},
}
